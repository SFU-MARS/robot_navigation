<launch>
  <!-- Arg to remap the scan topic -->
  <arg name="scan_topic" default="front/scan" />

  <!--- Run gmapping -->
  <include file="$(find husky_robot)/launch/gmapping.launch">
    <arg name="scan_topic" value="$(arg scan_topic)" />
  </include>

  <!--- Run AMCL -->
  <include file="$(find husky_robot)/launch/amcl.launch" />   

   <!--- Run Move Base -->  
  <include file="$(find husky_robot)/launch/move_base.launch">
    <arg name="no_static_map" value="true"/>
  </include>

</launch>

